<h1><%= @doctor.name %></h1>

<ul>
    <% @doctor.appointments.each do |appointment| %>
        <li>Patient: <%= link_to appointment.get_patient_name,
                patient_path(appointment.patient) %></li>
        <li>Time: <%= "#{ appointment.format_date_time[:date] } at " "#{ appointment.format_date_time[:time] }" %></li>
    <% end %>
</ul>
